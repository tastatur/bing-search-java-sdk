<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://schemas.microsoft.com/LiveSearch/2008/04/XML/multimedia" xmlns:mms="http://schemas.microsoft.com/LiveSearch/2008/04/XML/multimedia" xmlns:element="http://schemas.microsoft.com/LiveSearch/2008/04/XML/element" xmlns:web="http://schemas.microsoft.com/LiveSearch/2008/04/XML/web" xmlns:enc="http://schemas.microsoft.com/LiveSearch/2008/04/XML/encarta" xmlns:pho="http://schemas.microsoft.com/LiveSearch/2008/04/XML/phonebook" xmlns:ia="http://schemas.microsoft.com/LiveSearch/2008/04/XML/instantanswer" xmlns:rs="http://schemas.microsoft.com/LiveSearch/2008/04/XML/relatedsearch" xmlns:news="http://schemas.microsoft.com/LiveSearch/2008/04/XML/news" xmlns:ads="http://schemas.microsoft.com/LiveSearch/2008/04/XML/ads" xmlns:spl="http://schemas.microsoft.com/LiveSearch/2008/04/XML/spell" xmlns:tra="http://schemas.microsoft.com/LiveSearch/2008/04/XML/translation" xmlns:fls="http://schemas.microsoft.com/LiveSearch/2008/04/XML/element/flightstatus" xmlns:mw="http://schemas.microsoft.com/LiveSearch/2008/04/XML/mobileweb">
  <xs:import namespace="http://schemas.microsoft.com/LiveSearch/2008/04/XML/ads" schemaLocation="ads.xsd"/>
  <xs:import namespace="http://schemas.microsoft.com/LiveSearch/2008/04/XML/element" schemaLocation="bing-search-api.xsd"/>
  <xs:import namespace="http://schemas.microsoft.com/LiveSearch/2008/04/XML/element/flightstatus" schemaLocation="fls.xsd"/>
  <xs:import namespace="http://schemas.microsoft.com/LiveSearch/2008/04/XML/encarta" schemaLocation="enc.xsd"/>
  <xs:import namespace="http://schemas.microsoft.com/LiveSearch/2008/04/XML/instantanswer" schemaLocation="ia.xsd"/>
  <xs:import namespace="http://schemas.microsoft.com/LiveSearch/2008/04/XML/mobileweb" schemaLocation="mw.xsd"/>
  <xs:import namespace="http://schemas.microsoft.com/LiveSearch/2008/04/XML/news" schemaLocation="news.xsd"/>
  <xs:import namespace="http://schemas.microsoft.com/LiveSearch/2008/04/XML/phonebook" schemaLocation="pho.xsd"/>
  <xs:import namespace="http://schemas.microsoft.com/LiveSearch/2008/04/XML/relatedsearch" schemaLocation="rs.xsd"/>
  <xs:import namespace="http://schemas.microsoft.com/LiveSearch/2008/04/XML/spell" schemaLocation="spl.xsd"/>
  <xs:import namespace="http://schemas.microsoft.com/LiveSearch/2008/04/XML/translation" schemaLocation="tra.xsd"/>
  <xs:import namespace="http://schemas.microsoft.com/LiveSearch/2008/04/XML/web" schemaLocation="web.xsd"/>
  <xs:element name="Image">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="mms:Total"/>
        <xs:element ref="mms:Offset"/>
        <xs:element ref="mms:Results"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Video">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="mms:Total"/>
        <xs:element ref="mms:Offset"/>
        <xs:element ref="mms:Results"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Total" type="xs:integer"/>
  <xs:element name="Offset" type="xs:integer"/>
  <xs:element name="Results">
    <xs:complexType>
      <xs:choice>
        <xs:element maxOccurs="unbounded" ref="mms:ImageResult"/>
        <xs:element maxOccurs="unbounded" ref="mms:VideoResult"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="ImageResult">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="mms:Title"/>
        <xs:element ref="mms:MediaUrl"/>
        <xs:element ref="mms:Url"/>
        <xs:element ref="mms:DisplayUrl"/>
        <xs:element ref="mms:Width"/>
        <xs:element ref="mms:Height"/>
        <xs:element ref="mms:Thumbnail"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="MediaUrl" type="xs:anyURI"/>
  <xs:element name="DisplayUrl" type="xs:anyURI"/>
  <xs:element name="Thumbnail">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="mms:Url"/>
        <xs:element ref="mms:ContentType"/>
        <xs:element ref="mms:Width"/>
        <xs:element ref="mms:Height"/>
        <xs:element ref="mms:FileSize"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="VideoResult">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="mms:Title"/>
        <xs:element ref="mms:PlayUrl"/>
        <xs:element ref="mms:RunTime"/>
        <xs:element ref="mms:ClickThroughPageUrl"/>
        <xs:element ref="mms:StaticThumbnail"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="PlayUrl" type="xs:anyURI"/>
  <xs:element name="RunTime" type="xs:integer"/>
  <xs:element name="ClickThroughPageUrl" type="xs:anyURI"/>
  <xs:element name="StaticThumbnail">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="mms:Url"/>
        <xs:element ref="mms:ContentType"/>
        <xs:element ref="mms:Width"/>
        <xs:element ref="mms:Height"/>
        <xs:element ref="mms:FileSize"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Title" type="xs:string"/>
  <xs:element name="Url" type="xs:anyURI"/>
  <xs:element name="Width" type="xs:integer"/>
  <xs:element name="Height" type="xs:integer"/>
  <xs:element name="ContentType" type="xs:string"/>
  <xs:element name="FileSize" type="xs:integer"/>
</xs:schema>
